    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-flexbox-grid": "^0.10.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-tagsinput": "^3.14.0",
